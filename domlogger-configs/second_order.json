{
    "config": {
      "*": {
        "!match": [
          "facebook",
          "metamask",
          "wappalyzer",
          "google_tag_manager",
          "googletagmanager",
          "bitwarden",
          "messenger_awaiter",
          "grecaptcha",
          "hcaptcha"
        ],
        "!matchTrace": [
          "facebook",
          "metamask",
          "wappalyzer",
          "google_tag_manager",
          "googletagmanager",
          "bitwarden",
          "messenger_awaiter",
          "grecaptcha",
          "hcaptcha"
        ]
      },
      "storage": {
        "hideThis": true
      }
    },
    "hooks": {
      "NetworkSinks": {
        "class": [
          "WebSocket",
          "EventSource",
          "Request"
        ],
        "attribute": [
          "set:Request.prototype.body",
          "set:Request.prototype.headers"
        ],
        "function": [
          "fetch",
          "navigator.sendBeacon",
          "XMLHttpRequest.prototype.send",
          "XMLHttpRequest.prototype.open",
          "WebSocket.prototype.send",
          "XMLHttpRequest.prototype.setRequestHeader"
        ]
      },
      "NetworkSources": {
        "attribute": [
          "get:Response.prototype.body",
          "get:Response.prototype.headers",
          "get:XMLHttpRequest.prototype.response",
          "get:XMLHttpRequest.prototype.responseText",
          "get:XMLHttpRequest.prototype.responseXML"
        ],
        "function": [
          "Response.prototype.text",
          "Response.prototype.json",
          "Response.prototype.blob",
          "XMLHttpRequest.prototype.getAllResponseHeaders",
          "XMLHttpRequest.prototype.getResponseHeaders"
        ]
      },
      "misc": {
        "class": [
          "Blob"
        ],
        "function": [
          "URL.createObjectURL"
        ]
      },
      "storage": {
        "attribute": [
          "set:document.cookie",
          "get:document.cookie"
        ],
        "function": [
          "localStorage.getItem",
          "localStorage.setItem",
          "sessionStorage.getItem",
          "sessionStorage.setItem"
        ]
      }
    },
    "removeHeaders": [
      "content-security-policy"
    ]
  }